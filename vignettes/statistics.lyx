\end_layout

\begin_layout Plain Layout

# Find the mean number of spikes in 200ms bins starting from 1 second
\end_layout

\begin_layout Plain Layout

for(cell in rownames(wt.asp_fg.cVA)){
\end_layout

\begin_layout Plain Layout

  message("Summarising spikes for cell: ",cell)
\end_layout

